{
  "name": "YGG Plus Plus",
  "version": "1.1",
  "description": "Add downloads buttons and rating to ygg torrent",
  "manifest_version": 3,
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "all_frames": true,
      "js": ["yggplus.js"],
      "matches": [
        "https://yggtorrent.top/*",
        "https://www.yggtorrent.top/*",
        "https://www3.yggtorrent.top/*",
        "https://www5.yggtorrent.top/*",
        "https://www6.yggtorrent.top/*"
      ]
    }
  ]
}